 </parameter>
 <parameter type="simplekeyframe" name="Slope" factor="1000" keyframes="0=0" min="0" max="1000" value="0" default="0">
  <name>Slope</name>
 </parameter>
 <parameter type="list" name="Operation" paramlist="0.0;0.3;0.5;0.7;1.0" value="0.0" default="0.0">
  <paramlistdisplay>Write on clear,Max,Min,Add,Subtract</paramlistdisplay>
